<div class='card text-center text-bg-success mt-4'>
  {{#with service}}
    <div class='card-body'>
      <h2>{{title}}</h2>
      <p>{{description}}</p>
      {{#each buttons}}
        <a
          class='btn btn-primary'
          href='{{link}}'
          rel='{{rel}}'
        >{{text}} </a>
      {{/each}}
    </div>
  {{/with}}
</div>